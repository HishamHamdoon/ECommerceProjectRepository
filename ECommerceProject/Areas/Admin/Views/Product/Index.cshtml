@using Ecom.Models.Models;
@model IEnumerable<Product>
<div class="container">
    <div class="row pt-0">
        <div class="col-6">
            <h2 class="text-primary">  Product List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-action="Upsert" class="btn btn-primary"><i class="bi bi-plus-circle"></i>Create New Category</a>
        </div>
    </div>
</div>
 
<table id="tblData" class="table table-bordered">
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>ISBN</th>
            <th>Author</th>
            <th>Price</th>
            <th>Category</th>
            <th>List Price</th>
            
            <th></th>
         </tr>
    </thead>
 
</table>
@section Scripts{
    <script src="~/js/product.js"></script>
}